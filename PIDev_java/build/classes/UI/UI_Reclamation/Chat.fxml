<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.ChatController">
   <children>
      <VBox prefHeight="400.0" prefWidth="308.0">
         <children>
            <TextArea fx:id="taMessages" prefHeight="443.0" prefWidth="308.0" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="tfInput" prefHeight="29.0" prefWidth="242.0" />
                  <Button fx:id="btnSend" mnemonicParsing="false" onAction="#handleButtonSend" prefHeight="37.0" prefWidth="69.0" text="Envoyer" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
