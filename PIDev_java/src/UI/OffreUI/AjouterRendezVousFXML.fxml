<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.OffreUI.AjouterRendezVousFXMLController">
   <children>
      <VBox fx:id="calender" prefHeight="400.0" prefWidth="600.0">
